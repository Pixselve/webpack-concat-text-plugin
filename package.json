{
  "name": "concat-text-webpack-plugin",
  "version": "1.0.0",
  "description": "Concatenate and extract text files.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel index.js -d dist",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "Patrick Lam <patrick.lam@namics.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/namics/webpack-concat-text-plugin.git"
  },
  "dependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "concat": "1.0.3",
    "glob": "7.1.3",
    "webpack-sources": "1.3.0"
  },
  "devDependencies": {
    "@namics/eslint-config": "6.1.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "core-js": "3.0.1",
    "es6-promise": "4.2.6",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.17.2",
    "jest": "24.7.1",
    "memory-fs": "0.4.1",
    "webpack": "4.30.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testEnvironment": "node",
    "watchPathIgnorePatterns": [
      "<rootDir>/test/__output__"
    ],
    "coverageDirectory": "test/coverage",
    "collectCoverageFrom": [
      "*.js",
      "!**/node_modules/**",
      "!**/test/**"
    ]
  }
}
